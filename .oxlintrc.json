{
    "$schema": "./node_modules/oxlint/configuration_schema.json",
    "plugins": [
        "oxc",
        "eslint",
        "import",
        "typescript"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es2022": true
    },
    "globals": {
        "foo": "readonly"
    },
    "settings": {},
    "rules": {
        "eqeqeq": "warn",
        "import/no-cycle": "error"
    },
    "overrides": []
}
